    <sql>SELECT &#xa;    COALESCE&#x28;m.id_ministro, 0&#x29; as id_ministro,&#xa;    REPLACE&#x28;t.data, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS keydata,&#xa;    COALESCE&#x28;t.local_id_local, 0&#x29; as id_local,&#xa;    COALESCE&#x28;a.id_assunto, 0&#x29; AS id_assunto,&#xa;	CASE tm.sentimento_id_sentimento&#xa;		WHEN 1 THEN 1&#xa;		WHEN 0 THEN 0&#xa;		WHEN -1 THEN 0&#xa;	END as quantidade_positivos,&#xa;	CASE tm.sentimento_id_sentimento&#xa;		WHEN 1 THEN 0&#xa;		WHEN 0 THEN 1&#xa;		WHEN -1 THEN 0&#xa;	END as quantidade_neutros,&#xa;	CASE tm.sentimento_id_sentimento&#xa;		WHEN 1 THEN 0&#xa;		WHEN 0 THEN 0&#xa;		WHEN -1 THEN 1&#xa;	END as quantidade_negativos,&#xa;	tm.sentimento_id_sentimento as sentimento&#xa;FROM&#xa;    tweet t&#xa;        INNER JOIN&#xa;    tweet_ministro tm ON t.id_tweet &#x3d; tm.tweet_id_tweet&#xa;        INNER JOIN&#xa;    ministro m ON m.id_ministro &#x3d; tm.ministro_id_ministro&#xa;		LEFT JOIN&#xa;	tweet_assunto ta ON ta.tweet_id_tweet &#x3d; t.id_tweet&#xa;		LEFT JOIN&#xa;	assunto a ON a.id_assunto &#x3d; ta.assunto_id_assunto&#x3b;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
